{
  "name": "game-of-life",
  "version": "0.0.1",
  "description": "Conway's Game of Life in JavaScript",
  "author": "Brandon Semilla (https://github.com/semibran)",
  "repository": {
    "type": "git",
    "url": "https://github.com/semibran/game-of-life.git"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "npm start",
    "start": "npm-run-all --parallel serve watch",
    "serve": "reload -bd docs",
    "watch": "rollup --watch --input src/index.js -mco docs/build.js"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "npm-run-all": "^4.0.0",
    "reload": "^1.1.0",
    "rollup": "^0.41.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-watch": "^3.2.2"
  }
}
